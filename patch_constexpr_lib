+  constexpr std::array<int, 3> cam{{4, 5, 6}};
+  constexpr std::array<int, 3> camm{{-4, -5, -6}};
+
+  const auto outf = std::find_end(ca0.begin(), ca0.end(),
+				  cam.begin(), cam.end());
+
+  const auto outg = std::find_end(ca0.begin(), ca0.end(),
+				  camm.begin(), camm.end(),
+				  [](int i, int j){ return i + 1 == -j; });
+}
